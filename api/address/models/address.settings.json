{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "name": "Address",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "addressLine1": {
      "type": "text",
      "required": true
    },
    "addressLine2": {
      "type": "text"
    },
    "postalCode": {
      "required": true,
      "type": "integer"
    },
    "city": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string",
      "required": true
    },
    "addressName": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
